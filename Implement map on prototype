// this. method is used in this case for the prototype of the global variable

// The global variable
const s = [23, 65, 98, 5];

Array.prototype.myMap = function(callback) {
  const newArray = [];
  // Only change code below this line
for(let i = 0;i < this.length; i++){
  newArray.push(callback(this[i]))  // We passed the callback function for each indivudual element
}

  return newArray;
  
};

